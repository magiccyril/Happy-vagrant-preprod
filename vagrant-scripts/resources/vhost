<VirtualHost *:80>
	ServerAdmin webmaster@localhost

	DocumentRoot /var/www/vagrant/www

	<Directory /var/www/vagrant/www>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride All
		Require all granted
	</Directory>

	# Possible values include: debug, info, notice, warn, error, crit,
	# alert, emerg.
	LogLevel warn
</VirtualHost>
